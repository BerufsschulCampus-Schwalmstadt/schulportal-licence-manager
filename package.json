{
  "dependencies": {
    "concurrently": "^7.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11"
  },
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "npm run start --prefix ./backend",
    "devStart": "concurrently \"npm start --prefix ./client\" \"npm run dev --prefix ./server\"",
    "build": "npm install && npm install --prefix ./backend && npm run generate --prefix ./backend",
    "dev": "npm run start --prefix ./server",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "gts": "^3.1.1",
    "typescript": "~4.7.0"
  }
}
