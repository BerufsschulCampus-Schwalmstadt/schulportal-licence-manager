{
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "concurrently \"npm run start --prefix ./client\" \"npm run devStart --prefix ./server\"",
    "devStart": "concurrently \"npm start --prefix ./client\" \"npm run dev --prefix ./server\"",
    "build": "npm install && npm install --prefix ./client && cd ../ && npm install --prefix ./server",
    "dev": "npm run start --prefix ./server"
  }
}
