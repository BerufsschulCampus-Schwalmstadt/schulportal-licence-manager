{
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "npm run devStart --prefix ./server",
    "devStart": "concurrently \"npm start --prefix ./client\" \"npm run dev --prefix ./server\"",
    "build": "npm install && cd client && npm install -g serve l && serve -s build && cd ../ && npm install --prefix ./server",
    "dev": "npm run start --prefix ./server"
  }
}
